{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/$1.html" },
    { "source": "/(.*)", "destination": "/$1" }
  ],
  "buildCommand": "mkdir -p dist && cp -r * dist/ && rm -rf dist/dist",
  "outputDirectory": "dist",
  "framework": null
}